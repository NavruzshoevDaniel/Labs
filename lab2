#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#define MAXSHAB 17

int sdvig[256];
int shift = 0;

void table( unsigned char shablon[MAXSHAB]) {
    if (strlen(shablon) == 1)
        for (int i = 0; i < 256; i++)
            sdvig[i] = 1;
    else {
        for (int i = 0; i < 256; i++)
            sdvig[i] = strlen(shablon);
        for (int i = 0; i <= strlen(shablon) - 2; i++)
            sdvig[shablon[i]] = strlen(shablon) - (i + 1);
    }
}

void vhod( unsigned char shablon[MAXSHAB],  unsigned char inp[MAXSHAB], int blink) {
    int l = strlen(shablon), j = 0;
    j = l;
    if (inp[j - 1] != shablon[j - 1])
        printf("%d ", j + blink);
    else
        while ((j != 0) && (inp[j - 1] == shablon[j - 1])) {
            printf("%d ", j + blink);
            j--;
            if (j != 0)
                if (inp[j - 1] != shablon[j - 1])
                    printf("%d ", j + blink);
        }
    shift = sdvig[inp[l - 1]];
}

int main()
{
    unsigned char  shablon[MAXSHAB] = { 0 }, inp[MAXSHAB] = { 0 };
    char k;
    int i = 0, blink = 0;
    gets(shablon);
    table(shablon);
    while ((k = getchar()) != EOF){
        inp[i] = k;
        i++;
        if (i == strlen(shablon)){
            vhod (shablon, inp, blink);
            blink += shift;
            memmove (inp, inp + shift, strlen(inp) - shift);
            i -= shift;
        }
    }
    return 0;
}
